{
  "framework": "nextjs",
  "regions": ["iad1"],
  "functions": {
    "app/api/pos/payment/process/route.ts": {
      "maxDuration": 60
    },
    "app/api/pos/payment-processors/health/route.ts": {
      "maxDuration": 30
    },
    "app/api/ai/**/*.ts": {
      "maxDuration": 60
    },
    "app/api/vendor/marketing/**/*.ts": {
      "maxDuration": 60
    },
    "app/api/vendor/campaigns/**/*.ts": {
      "maxDuration": 60
    },
    "app/api/vendor/media/**/*.ts": {
      "maxDuration": 60
    },
    "app/api/**/*.ts": {
      "maxDuration": 30
    }
  },
  "env": {
    "AUTHORIZENET_API_LOGIN_ID": "YOUR_API_LOGIN_ID",
    "AUTHORIZENET_TRANSACTION_KEY": "YOUR_TRANSACTION_KEY",
    "AUTHORIZENET_SIGNATURE_KEY": "YOUR_SIGNATURE_KEY",
    "AUTHORIZENET_ENVIRONMENT": "production",
    "NEXT_PUBLIC_AUTHORIZENET_CLIENT_KEY": "YOUR_PUBLIC_CLIENT_KEY",
    "NEXT_PUBLIC_AUTHORIZENET_API_LOGIN_ID": "YOUR_API_LOGIN_ID",
    "NEXT_PUBLIC_AUTHORIZENET_ENVIRONMENT": "production"
  }
}
