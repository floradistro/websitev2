{
  "name": "whaletools",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3000 -H 0.0.0.0",
    "dev:fresh": "rm -rf .next && next dev -p 3000",
    "dev:https": "concurrently \"next dev --turbopack -p 3000\" \"local-ssl-proxy --source 3443 --target 3000\"",
    "build": "next build",
    "build:fresh": "rm -rf .next && next build",
    "start": "next start -p 3000",
    "postinstall": "echo 'Install complete'",
    "generate:smart-component": "npx tsx scripts/smart-component-generator.ts",
    "generate:admin-hash": "npx tsx scripts/generate-admin-hash.ts",
    "cleanup:console-logs": "npx tsx scripts/remove-console-logs.ts",
    "cleanup:console-logs-aggressive": "npx tsx scripts/remove-console-logs-aggressive.ts",
    "db:register": "ts-node scripts/register-component.ts",
    "db:info": "npx tsx --env-file=.env.local scripts/supabase-cli.ts info",
    "db:migrate": "npx tsx --env-file=.env.local scripts/supabase-cli.ts migrate",
    "db:query": "npx tsx --env-file=.env.local scripts/supabase-cli.ts query",
    "db:exec": "npx tsx --env-file=.env.local scripts/supabase-cli.ts exec",
    "test:atomic": "npx tsx --env-file=.env.local scripts/test-all-atomic-operations.ts",
    "test:edge-cases": "npx tsx --env-file=.env.local scripts/test-edge-cases.ts",
    "cleanup": "bash scripts/cleanup-docs.sh && prettier --write \"**/*.{ts,tsx,js,jsx,json,css,md}\" && eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "cleanup:build": "rm -rf .next && rm -f tsconfig.tsbuildinfo && rm -rf playwright-report && rm -rf test-results",
    "cleanup:full": "bash scripts/cleanup.sh",
    "cleanup:docs": "bash scripts/cleanup-docs.sh",
    "db:optimize": "bash scripts/nightly-db-optimize.sh",
    "analyze": "node scripts/analyze-performance.js",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "type-check": "tsc --noEmit",
    "validate:no-bugs": "bash scripts/validate-no-disappearing-bugs.sh",
    "nightly": "npm run cleanup && npm run db:optimize && npm run analyze",
    "prepare": "husky",
    "roadmap": "npx tsx scripts/roadmap-tracker.ts",
    "roadmap:update": "npx tsx scripts/roadmap-tracker.ts update",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "test:pwa": "playwright test --config=playwright.pwa.config.ts",
    "test:pwa:ui": "playwright test --config=playwright.pwa.config.ts --ui",
    "test:pwa:report": "playwright show-report playwright-report/pwa",
    "db:analyze-queries": "npx tsx scripts/analyze-query-patterns.ts",
    "db:show-indexes": "echo 'Run this SQL in Supabase: SELECT tablename, indexname FROM pg_indexes WHERE indexname LIKE ''idx_%'' ORDER BY tablename;'",
    "db:test-connections": "npx tsx scripts/test-db-connections.ts",
    "cache:test-performance": "npx tsx scripts/test-cache-performance.ts"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.67.1",
    "@babel/standalone": "^7.28.5",
    "@ducanh2912/next-pwa": "^10.2.9",
    "@headlessui/react": "^2.2.9",
    "@monaco-editor/react": "^4.7.0",
    "@octokit/rest": "^22.0.0",
    "@react-email/components": "^1.0.0",
    "@react-email/render": "^2.0.0",
    "@react-pdf/renderer": "^4.3.1",
    "@sentry/nextjs": "^10.23.0",
    "@supabase/supabase-js": "^2.76.0",
    "@tanstack/react-query": "^5.90.6",
    "@tanstack/react-query-devtools": "^5.90.2",
    "@upstash/redis": "^1.35.6",
    "@vercel/edge": "^1.2.2",
    "@zxing/browser": "^0.1.5",
    "@zxing/library": "^0.21.3",
    "authorizenet": "^1.0.10",
    "axios": "^1.12.2",
    "bcryptjs": "^3.0.3",
    "cloudinary": "^2.8.0",
    "critters": "^0.0.23",
    "csv-parse": "^6.1.0",
    "decimal.js": "^10.6.0",
    "exa-js": "^1.10.2",
    "expo": "^54.0.23",
    "form-data": "^4.0.4",
    "framer-motion": "^12.23.24",
    "gsap": "^3.13.0",
    "lru-cache": "^11.2.2",
    "lucide-react": "^0.545.0",
    "nanoid": "^5.1.6",
    "next": "^15.5.7",
    "p5": "^2.0.5",
    "passkit-generator": "^3.5.5",
    "playwright": "^1.56.1",
    "qrcode.react": "^4.2.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-intersection-observer": "^9.16.0",
    "react-is": "^19.1.0",
    "react-markdown": "^10.1.0",
    "recharts": "^3.4.1",
    "resend": "^6.4.2",
    "sharp": "^0.34.4",
    "swr": "^2.3.6",
    "xlsx": "^0.18.5",
    "zod": "^4.1.12",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^16.0.0",
    "@playwright/test": "^1.56.1",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.19.23",
    "@types/p5": "^1.7.7",
    "@types/pg": "^8.15.5",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "dotenv": "^17.2.3",
    "eslint": "^8.57.1",
    "eslint-config-next": "^16.0.1",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "lint-staged": "^16.2.6",
    "local-ssl-proxy": "^2.0.5",
    "node-fetch": "^2.7.0",
    "pg": "^8.16.3",
    "prettier": "^3.6.2",
    "tailwindcss": "^4",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "^5"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  }
}
