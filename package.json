{
  "name": "whaletools",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3000",
    "dev:https": "concurrently \"next dev --turbopack -p 3000\" \"local-ssl-proxy --source 3443 --target 3000\"",
    "build": "next build",
    "start": "next start -p 3000",
    "postinstall": "echo 'Install complete'",
    "generate:smart-component": "npx tsx scripts/smart-component-generator.ts",
    "generate:admin-hash": "npx tsx scripts/generate-admin-hash.ts",
    "cleanup:console-logs": "npx tsx scripts/remove-console-logs.ts",
    "cleanup:console-logs-aggressive": "npx tsx scripts/remove-console-logs-aggressive.ts",
    "db:register": "ts-node scripts/register-component.ts",
    "cleanup": "bash scripts/cleanup-docs.sh && prettier --write \"**/*.{ts,tsx,js,jsx,json,css,md}\" && eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "cleanup:docs": "bash scripts/cleanup-docs.sh",
    "db:optimize": "bash scripts/nightly-db-optimize.sh",
    "analyze": "node scripts/analyze-performance.js",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "type-check": "tsc --noEmit",
    "nightly": "npm run cleanup && npm run db:optimize && npm run analyze",
    "prepare": "husky"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.67.1",
    "@codesandbox/sandpack-react": "^2.20.0",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@headlessui/react": "^2.2.9",
    "@monaco-editor/react": "^4.7.0",
    "@octokit/rest": "^22.0.0",
    "@scandit/web-datacapture-barcode": "^7.6.3",
    "@scandit/web-datacapture-core": "^7.6.3",
    "@scandit/web-datacapture-id": "^7.6.3",
    "@supabase/supabase-js": "^2.76.0",
    "@tanstack/react-query": "^5.90.6",
    "@tanstack/react-query-devtools": "^5.90.2",
    "@types/handlebars": "^4.0.40",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@types/react-window": "^1.8.8",
    "@types/three": "^0.180.0",
    "@upstash/redis": "^1.35.6",
    "@vercel/edge": "^1.2.2",
    "authorizenet": "^1.0.10",
    "axios": "^1.12.2",
    "bcryptjs": "^3.0.3",
    "cloudinary": "^2.8.0",
    "critters": "^0.0.23",
    "csv-parse": "^6.1.0",
    "exa-js": "^1.10.2",
    "form-data": "^4.0.4",
    "framer-motion": "^12.23.24",
    "gsap": "^3.13.0",
    "lru-cache": "^11.2.2",
    "lucide-react": "^0.545.0",
    "mime-types": "^3.0.1",
    "next": "15.5.5",
    "p5": "^2.0.5",
    "passkit-generator": "^3.5.5",
    "playwright": "^1.56.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-intersection-observer": "^9.16.0",
    "react-is": "^19.1.0",
    "react-markdown": "^10.1.0",
    "react-syntax-highlighter": "^16.1.0",
    "react-window": "^2.2.3",
    "recharts": "^3.3.0",
    "remark-gfm": "^4.0.1",
    "sharp": "^0.34.4",
    "swr": "^2.3.6",
    "xlsx": "^0.18.5",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^16.0.0",
    "@playwright/test": "^1.56.1",
    "@tailwindcss/postcss": "^4",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20.19.23",
    "@types/p5": "^1.7.7",
    "@types/pg": "^8.15.5",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "dotenv": "^17.2.3",
    "eslint": "^8.57.1",
    "eslint-config-next": "^16.0.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
    "local-ssl-proxy": "^2.0.5",
    "node-fetch": "^2.7.0",
    "pg": "^8.16.3",
    "prettier": "^3.6.2",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "^5"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  }
}
